#!/bin/bash
sudo pacman -S rofi waybar otf-font-awesome grim network-manager-applet nautilus hyprpaper
sudo cp waybar/* /etc/xdg/waybar/
rm -rf ~/.config/hypr/*
sudo cp hyprland/* ~/.config/hypr/
sudo rm -rf /usr/share/rofi/themes/sidebar-v2.rasi
sudo cp rofi/sidebar-v2.rasi /usr/share/rofi/themes/
sudo rm -rf ~/ro
sudo cp rofi/config.rasi ~/.config/rofi/yy
